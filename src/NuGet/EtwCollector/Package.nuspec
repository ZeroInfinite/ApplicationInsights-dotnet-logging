    <file src="$configuration$\src\Adapters\Etw.Net451\Microsoft.ApplicationInsights.EtwCollector.dll" target="lib\net451" />
    <file src="$configuration$\src\Adapters\Etw.Net451\Microsoft.ApplicationInsights.EtwCollector.pdb" target="lib\net451" />
    <file src="$configuration$\src\Adapters\Etw.Net451\Microsoft.ApplicationInsights.EtwCollector.xml" target="lib\net451" />
    <!-- XDT transformations -->
    <file src="$configuration$\src\Nuget\EtwCollector\ApplicationInsights.config.install.xdt" target="content"/>
